



{
	"compileOnSave": true,
	"compilerOptions": {
		"allowUnreachableCode": false,
		"alwaysStrict": true,
		"declaration": false,
		"diagnostics": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [
			"es2017",
			"dom"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"removeComments": false,
		"skipDefaultLibCheck": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"suppressImplicitAnyIndexErrors": true,
		"target": "es5",
		"typeRoots": [
			"node_modules/@types"
		],
		"types": [
			"node"
		]
	},
	"include": [
		"Angular"
	],
	"exclude": [
		"Other",
		"**/*.spec.ts"
	]
}


